{# templates/form.html.twig #}

{% extends 'base.html.twig' %}

{% block body %}
{%form_theme form 'bootstrap_4_layout.html.twig'%}
    <h1>Créer un utilisateur</h1>

    {{ form_start(form) }}
    
        {{ form_row(form.email) }}
        {{ form_row(form.roles) }}
        {{ form_row(form.password) }}
        {{ form_row(form.NomC) }}

        <button type="submit">Créer</button>
    {{ form_end(form) }}
{% endblock %}
